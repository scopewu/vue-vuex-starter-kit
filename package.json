{
  "name": "vue-vuex-starter-kit",
  "version": "0.2.0",
  "description": "Get started with Vue, Vuex, and Vue-router. Build app with webpack.",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "clean": "rimraf dist",
    "clean:dll": "rimraf dll",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "server": "better-npm-run server",
    "server:prod": "better-npm-run server:prod"
  },
  "betterScripts": {
    "start": {
      "command": "node ./server/bin/www",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "node ./server/bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run compile",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "server": {
      "command": "node ./server/bin/www",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "server:prod": {
      "command": "npm run deploy:prod && node ./server/bin/www",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scopewu/vue-vuex-starter-kit.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "vue-start-kit"
  ],
  "author": "scope.wu <scope.wu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scopewu/vue-vuex-starter-kit/issues"
  },
  "homepage": "https://github.com/scopewu/vue-vuex-starter-kit#readme",
  "dependencies": {
    "axios": "^0.16.0",
    "better-npm-run": "0.0.15",
    "connect-history-api-fallback": "^1.3.0",
    "core-js": "^2.4.1",
    "debug": "^2.6.3",
    "express": "^4.15.2",
    "ie-shim": "^0.1.0",
    "normalize.css": "^6.0.0",
    "vue": "^2.2.6",
    "vue-router": "^2.3.1",
    "vuex": "^2.3.0"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.0.1",
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.2",
    "optimize-js-plugin": "0.0.4",
    "postcss-loader": "^1.3.3",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "vue-loader": "^11.3.4",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  }
}
