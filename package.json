{
  "name": "vue-vuex-starter-kit",
  "version": "0.5.0",
  "description": "Get started with Vue, Vuex, and Vue-router. Build app with webpack.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env env.DEBUG=app:* node ./server/bin/www",
    "clean": "rimraf dist",
    "clean:dll": "rimraf dll",
    "compile": "npm run lint && cross-env env.DEBUG=app:* node ./server/bin/compile",
    "deploy": "cross-env env.DEBUG=app:* node ./server/bin/compile",
    "deploy:dev": "cross-env env.DEBUG=app:* env.NODE_ENV=development node ./server/bin/compile",
    "deploy:prod": "cross-env env.DEBUG=app:* env.NODE_ENV=production node ./server/bin/compile",
    "lint": "cross-env env.NODE_ENV=production eslint --ext .js,.vue src",
    "server": "cross-env env.DEBUG=app:* node ./server/bin/www",
    "server:prod": "cross-env env.DEBUG=app:* env.NODE_ENV=production node ./server/bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scopewu/vue-vuex-starter-kit.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "vue-start-kit"
  ],
  "author": "scope.wu <scope.wu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scopewu/vue-vuex-starter-kit/issues"
  },
  "homepage": "https://github.com/scopewu/vue-vuex-starter-kit#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "connect-history-api-fallback": "^1.3.0",
    "debug": "^2.6.8",
    "express": "^4.15.2",
    "vue": "^2.4.1",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.0"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.0.1",
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "eslint": "^4.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "optimize-js-plugin": "0.0.4",
    "postcss-loader": "^1.3.3",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-loader": "^12.2.2",
    "vue-template-compiler": "^2.4.1",
    "webpack": "^3.2.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  }
}
