{
  "name": "vue-vuex-starter-kit",
  "version": "0.6.0",
  "description": "Get started with Vue, Vuex, and Vue-router. Build app with webpack.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env env.DEBUG=app:* node ./server/bin/www",
    "clean": "rimraf dist",
    "clean:dll": "rimraf dll",
    "compile": "npm run lint && cross-env env.DEBUG=app:* node ./server/bin/compile",
    "deploy": "cross-env env.DEBUG=app:* node ./server/bin/compile",
    "deploy:dev": "cross-env env.DEBUG=app:* env.NODE_ENV=development node ./server/bin/compile",
    "deploy:prod": "cross-env env.DEBUG=app:* env.NODE_ENV=production node ./server/bin/compile",
    "lint": "cross-env env.NODE_ENV=production eslint --ext .js,.vue src",
    "server": "cross-env env.DEBUG=app:* node ./server/bin/www",
    "server:prod": "cross-env env.DEBUG=app:* env.NODE_ENV=production node ./server/bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scopewu/vue-vuex-starter-kit.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "vue-start-kit"
  ],
  "author": "scope.wu <scope.wu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scopewu/vue-vuex-starter-kit/issues"
  },
  "homepage": "https://github.com/scopewu/vue-vuex-starter-kit#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "connect-history-api-fallback": "^1.3.0",
    "debug": "^3.1.0",
    "express": "^4.15.2",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/polyfill": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-stage-1": "^7.0.0-beta.40",
    "@babel/preset-stage-2": "^7.0.0-beta.40",
    "assets-webpack-plugin": "^3.5.1",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^8.0.0-beta.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.9",
    "cssnano": "^3.10.0",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^1.3.3",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1"
  }
}
